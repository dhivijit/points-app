# Copy this file to .env and update the values for your environment

# MongoDB connection string. For local MongoDB use something like:
# MONGODB_URI=mongodb://localhost:27017/points
MONGODB_URI=

# JWT secret for signing auth cookies. Use a long random string in production.
JWT_SECRET=your-strong-random-secret

# Admin password for the simple login form. Change to a secure password.
PASSWORD=adminpass

# Optional: port to run the app on (default 3000)
# PORT=3000
